{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4",
    "ext-apcu": "*",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mongodb": "^1.9",
    "anik/elastic-apm-php": "^2.1",
    "aws/aws-sdk-php": "^3.184",
    "bandwidth/sdk": "^5.1",
    "barryvdh/laravel-dompdf": "^2.0",
    "box/spout": "^3.2",
    "cviebrock/eloquent-sluggable": "6.0.*",
    "doctrine/dbal": "^2.10",
    "dompdf/dompdf": "^2.0",
    "fideloper/proxy": "^4.0",
    "firebase/php-jwt": "^5.2",
    "fruitcake/laravel-cors": "^2.0",
    "grohiro/laravel-blade-macro": "^4.0",
    "guzzlehttp/guzzle": "^7.3",
    "infusionsoft/php-sdk": "^1.5",
    "intervention/image": "^2.5",
    "laravel/framework": "^6.2",
    "laravel/helpers": "^1.1",
    "laravel/tinker": "^1.0",
    "laravelcollective/html": "^6.1",
    "launchdarkly/server-sdk": "^4.2",
    "launchdarkly/server-sdk-redis-predis": "^1.2",
    "lcobucci/jwt": "3.3.3",
    "league/csv": "9.0",
    "league/flysystem-aws-s3-v3": "^1.0",
    "lucascudo/laravel-pt-br-localization": "^1.1",
    "maatwebsite/excel": "3.1.33",
    "mews/purifier": "^3.3",
    "mongodb/mongodb": "^1.6",
    "mpdf/mpdf": "^8.0",
    "mundipagg/mundiapi": "^3.1.14",
    "pagarme/pagarme-php": "^4.1",
    "predis/predis": "^1.1",
    "respect/validation": "^2.2",
    "sentry/sentry-laravel": "^2.3",
    "sopamo/laravel-filepond": "^0.6.2",
    "spatie/laravel-cors": "^1.6",
    "staudenmeir/laravel-upsert": "^1.0",
    "tecnickcom/tcpdf": "^6.4",
    "tymon/jwt-auth": "1.0.0-rc.5",
    "vkovic/laravel-custom-casts": "^1.4",
    "webpatser/laravel-uuid": "^3.0",
    "yajra/laravel-datatables-oracle": "^9.17"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.6",
    "facade/ignition": "^1.4",
    "fzaninotto/faker": "^1.4",
    "laravel/ui": "^1.0",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^3.0",
    "orchestra/testbench": "~4.0",
    "phpunit/phpunit": "^8.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
        "php-http/discovery": true
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Modules\\": "modules/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
