{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel",
        "laravel8"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "box/spout": "^3.3",
        "codedge/laravel-fpdf": "^1.8",
        "cviebrock/eloquent-sluggable": "^8.0.8",
        "doctrine/dbal": "^2.13.2",
        "elasticsearch/elasticsearch": "v6.7.2",
        "fideloper/proxy": "^4.4.1",
        "firebase/php-jwt": "^6.3",
        "fruitcake/laravel-cors": "^v2.0.4",
        "grohiro/laravel-blade-macro": "^4.0",
        "guzzlehttp/guzzle": "^7.3.0",
        "intervention/image": "^2.7",
        "laravel/framework": "^v8.55.0",
        "laravel/helpers": "^1.4",
        "laravel/legacy-factories": "^1.1",
        "laravel/tinker": "^2.6.1",
        "laravel/ui": "^3.3.0",
        "laravelcollective/html": "^6.1.1",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maennchen/zipstream-php": "^2.1",
        "mundipagg/mundiapi": "^3.1.20",
        "nelexa/zip": "^4.0",
        "phpoffice/phpspreadsheet": "^1.22",
        "predis/predis": "^1.1.7",
        "respect/validation": "^2.2",
        "rogierw/rw-acme-client": "^1.1.0",
        "sentry/sentry-laravel": "^2.10",
        "symfony/psr-http-message-bridge": "^2.1.1",
        "tymon/jwt-auth": "dev-develop",
        "webpatser/laravel-uuid": "^3.0.2",
        "zanysoft/laravel-zip": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6.2",
        "ergebnis/composer-normalize": "^2.15.0",
        "facade/ignition": "^2.11.4",
        "fzaninotto/faker": "^1.9.2",
        "jms/composer-deps-analyzer": "^1.0.1",
        "laravel/telescope": "^4.6",
        "mockery/mockery": "^1.4.3",
        "nette/php-generator": "^3.6",
        "nikic/php-parser": "^4.13",
        "nunomaduro/collision": "^5.8.0",
        "php-parallel-lint/php-console-highlighter": "v0.5",
        "phpunit/phpunit": "^9.5.8",
        "pyrech/composer-changelogs": "^1.7.1",
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "^3.6.0",
        "symfony/var-dumper": "^5.3.6"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "pyrech/composer-changelogs": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": ["laravel/telescope"]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "php ./resources/fix/fix.php"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
