{
  "name": "xgrow-api-integration",
  "version": "1.0.35",
  "description": "",
  "main": "src/app.js",
  "scripts": {
    "start": "npm run build && npm run consumer",
    "consumer": "node -r dotenv/config dist/src/app.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "test": "export APP_ENVIRONMENT=test && node --trace-warnings -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --passWithNoTests --maxWorkers=4 --no-cache --detectOpenHandles --forceExit",
    "test:cov": "npm run test -- --coverage --config ./jest-unit.config.js",
    "test:e2e": "npm run test -- --config ./jest-e2e.config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "changelog": "auto-changelog -p -u --commit-limit false --hide-credit --template changelog-template.hbs",
    "changelog:patch": "npm version patch && auto-changelog -p -u --commit-limit false --hide-credit --template changelog-template.hbs",
    "changelog:minor": "npm version minor && auto-changelog -p -u --commit-limit false --hide-credit --template changelog-template.hbs",
    "changelog:major": "npm version major && auto-changelog -p -u --commit-limit false --hide-credit --template changelog-template.hbs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bull": "^3.15.2",
    "@types/jest": "^27.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.4.1",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "auto-changelog": "^2.4.0",
    "eslint": "^7.31.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "git-commit-msg-linter": "^3.2.6",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^11.1.0",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bullmq": "^3.10.0",
    "dotenv": "^10.0.0",
    "elastic-apm-node": "^3.44.0",
    "expo-server-sdk": "^3.6.0",
    "form-data": "^4.0.0",
    "joi": "^17.7.1",
    "moment": "^2.29.4",
    "mongoose": "^5.13.7",
    "mongoose-timezone": "^1.3.0",
    "rimraf": "^3.0.2",
    "toad-scheduler": "^2.2.0",
    "uuid": "^9.0.0",
    "winston": "^3.3.3"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
