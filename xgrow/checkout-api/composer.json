{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-curl": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mongodb": "^1.9",
        "anik/elastic-apm-php": "^2.1",
        "aws/aws-sdk-php": "^3.184",
        "bandwidth/sdk": "^5.1",
        "box/spout": "^3.2",
        "bulkgate/php-sdk": "^1.0",
        "cviebrock/eloquent-sluggable": "^8.0",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.0",
        "firebase/php-jwt": "^5.2",
        "fruitcake/laravel-cors": "^2.0",
        "google/cloud-pubsub": "^1.39",
        "google/recaptcha": "^1.2",
        "guzzlehttp/guzzle": "^7.3",
        "infusionsoft/php-sdk": "^1.5",
        "laravel/framework": "^8.69",
        "laravel/helpers": "^1.1",
        "laravel/legacy-factories": "^1.1",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "laravelcollective/html": "^6.1",
        "lcobucci/jwt": "3.3.3",
        "league/flysystem-aws-s3-v3": "^1.0",
        "mongodb/mongodb": "^1.6",
        "mundipagg/mundiapi": "^3.1.14",
        "pagarme/pagarme-php": "^4.1",
        "pagarme/pagarmecoreapi": "^5.7",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^2.3",
        "tymon/jwt-auth": "^1.0",
        "webpatser/laravel-uuid": "^3.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "google/cloud-functions-framework": "^1.1",
        "jobcloud/avro-validator": "^1.0",
        "laravel/sail": "^1.0.1",
        "lucascudo/laravel-pt-br-localization": "^1.2",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "orchestra/testbench": "^6.22",
        "phpunit/phpunit": "^9.5.10"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
	"allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
