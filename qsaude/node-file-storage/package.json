{
  "name": "node-file-storage",
  "version": "0.3.0",
  "description": "NestJS File Storage Microservice",
  "author": "QSa√∫de<contato@qsaude.com.br",
  "private": true,
  "license": "UNLICENSED",
  "repository": "https://QsaudeDevOps@dev.azure.com/QsaudeDevOps/DigitalWorkPlace/_git/node-file-storage",
  "contributors": [
    {
      "name": "Mateus Macedo Dos Anjos",
      "email": "mateus.capitani@qsaude.com.br"
    }
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "export NODE_ENV=production && nest start",
    "start:win": "cross-env NODE_ENV=development && nest start",
    "start:dev": "export NODE_ENV=development && nest start --watch",
    "start:dev:win": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "export NODE_ENV=development && nest start --debug --watch",
    "start:debug:win": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "export NODE_ENV=production && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:staged": "eslint --fix",
    "test": "export NODE_ENV=test && node --trace-warnings -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --passWithNoTests --runInBand --no-cache --detectOpenHandles --forceExit",
    "test:win": "cross-env NODE_ENV=test node --trace-warnings -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --passWithNoTests --runInBand --no-cache --detectOpenHandles --forceExit",
    "test:unit": "npm run test -- --config ./jest-unit.config.js",
    "test:win:unit": "cross-env NODE_ENV=test npm run test:win -- --config ./jest-unit.config.js",
    "test:staged": "npm run test -- --config ./jest-unit.config.js --findRelatedTests",
    "test:win:staged": "cross-env NODE_ENV=test npm run test:win -- --config ./jest-unit.config.js --findRelatedTests",
    "test:watch": "npm run test -- --watch --config ./jest-unit.config.js",
    "test:win:watch": "cross-env NODE_ENV=test npm run test:win -- --watch --config ./jest-unit.config.js",
    "test:cov": "npm run test -- --coverage --config ./jest-unit.config.js",
    "test:win:cov": "npm run test:win -- --coverage --config ./jest-unit.config.js",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --passWithNoTests --runInBand --no-cache --config ./jest-unit.config.js",
    "test:win:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --passWithNoTests --runInBand --no-cache --config ./jest-unit.config.js",
    "test:e2e": "npm run test -- --config ./jest-e2e.config.js",
    "test:win:e2e": "cross-env NODE_ENV=test npm run test:win -- --config ./jest-e2e.config.js",
    "prepare": "husky install",
    "snyk": "snyk test",
    "changelog": "auto-changelog -p -u --commit-limit false --hide-credit --template changelog-template.hbs",
    "changelog:minor": "standard-version --release-as minor && auto-changelog -p -u --commit-limit false --hide-credit --template changelog-template.hbs",
    "changelog:major": "standard-version --release-as major && auto-changelog -p -u --commit-limit false --hide-credit --template changelog-template.hbs",
    "changelog:patch": "standard-version --release-as patch && auto-changelog -p -u --commit-limit false --hide-credit --template changelog-template.hbs",
    "changelog:alpha": "standard-version --prerelease alpha && auto-changelog -p -u --commit-limit false --hide-credit --template changelog-template.hbs",
    "sonar-scanner": "sonar-scanner"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.81.0",
    "@aws-sdk/client-sns": "^3.82.0",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/event-emitter": "^1.1.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/terminus": "^8.0.6",
    "@nestjs/throttler": "^2.0.1",
    "change-case": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "helmet": "^5.0.2",
    "joi": "^17.6.0",
    "mime-types": "^2.1.35",
    "nestjs-i18n": "^9.0.10",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2",
    "@metinseylan/nestjs-opentelemetry": "^2.0.7",
    "@nestjs/passport": "^8.2.2",
    "@opentelemetry/auto-instrumentations-node": "0.27.4",
    "@opentelemetry/context-async-hooks": "1.0.1",
    "@opentelemetry/core": "1.0.1",
    "@opentelemetry/exporter-jaeger": "1.0.1",
    "@opentelemetry/exporter-prometheus": "0.27.0",
    "@opentelemetry/instrumentation-aws-sdk": "0.6.0",
    "@opentelemetry/instrumentation-connect": "0.27.0",
    "@opentelemetry/instrumentation-dns": "0.27.0",
    "@opentelemetry/instrumentation-express": "0.27.0",
    "@opentelemetry/instrumentation-generic-pool": "0.27.0",
    "@opentelemetry/instrumentation-http": "0.27.0",
    "@opentelemetry/instrumentation-ioredis": "0.27.0",
    "@opentelemetry/instrumentation-nestjs-core": "0.28.3",
    "@opentelemetry/instrumentation-net": "0.27.0",
    "@opentelemetry/instrumentation-pino": "0.28.1",
    "@opentelemetry/propagator-b3": "1.0.1",
    "@opentelemetry/propagator-jaeger": "1.0.1",
    "@opentelemetry/sdk-trace-base": "1.0.1",
    "jwks-rsa": "^2.1.4",
    "nestjs-pino": "^2.6.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pino-http": "^7.1.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.78.0",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@golevelup/nestjs-testing": "^0.1.2",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@jest-mock/express": "^1.4.5",
    "@types/eslint": "^8.4.2",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/helmet": "^4.0.0",
    "@types/joi": "^17.2.3",
    "@types/passport-jwt": "^3.0.6",
    "@types/pino-http": "^5.8.1",
    "@types/prettier": "^2.6.3",
    "@types/reflect-metadata": "^0.1.0",
    "@types/rimraf": "^3.0.2",
    "@types/source-map-support": "^0.5.4",
    "@types/standard-version": "^7.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/typescript": "^2.0.0",
    "auto-changelog": "^2.4.0",
    "aws-sdk-client-mock": "^0.6.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "faker-br": "^0.4.1",
    "get-stream": "^6.0.1",
    "git-commit-msg-linter": "^4.1.1",
    "git-cz": "^4.8.0",
    "husky": "^7.0.4",
    "jest": "^27.2.5",
    "jest-aws-simple-mock": "^0.11.5",
    "jest-mock-extended": "^2.0.5",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^12.3.7",
    "prettier": "^2.3.2",
    "snyk": "^1.861.0",
    "sonarqube-scanner": "^2.8.1",
    "source-map-support": "^0.5.20",
    "standard-version": "^9.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "standard-version": {
    "skip": {
      "changelog": true,
      "commit": true
    }
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "coverage-final.xml",
    "indent": 4
  }
}
