{
  "name": "boileirplate-sls",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test && node --trace-warnings -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --passWithNoTests --maxWorkers=4 --no-cache --detectOpenHandles --forceExit",
    "test:unit": "npm run test -- --config ./jest-unit.config.js",
    "test:staged": "npm run test -- --config ./jest-unit.config.js --findRelatedTests",
    "test:watch": "npm run test -- --watch --config ./jest-unit.config.js",
    "test:cov": "npm run test -- --coverage --config ./jest-unit.config.js",
    "prepare-husky": "husky install",
    "start": "sls offline",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.build.json",
    "sls:remove": "sls remove",
    "sls:deploy": "npm run build && sls deploy",
    "sls:deploy:prod": "npm run build && sls deploy --stage production"
  },
  "dependencies": {
    "@sentry/serverless": "^7.61.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.119",
    "@types/fhir": "^0.0.37",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "jest": "29.5.0",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^2.3.2",
    "serverless": "^3.33.0",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-typescript": "^2.1.5",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  }
}
